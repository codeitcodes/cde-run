{
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/_/run/",
      "destination": "https://dev.codeit.codes/full"
    },
    {
      "source": "/_/run/:path*",
      "destination": "https://dev.codeit.codes/:path*"
    }
  ],
  "redirects": [
      {
        "source": "/",
        "destination": "https://dev.codeit.codes/",
        "permanent": true
      },
      {
        "source": "/:path((?!_/).*)",
        "destination": "https://dev.codeit.codes/api/link?url=:path*",
        "permanent": true
      },
      {
        "source": "/:path((?!_/).*)/",
        "destination": "https://dev.codeit.codes/api/link?url=:path*",
        "permanent": true
      }
   ]
}
