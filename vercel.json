{
  "github": {
    "silent": true
  },
  "redirects": [
      {
        "source": "/",
        "destination": "https://dev.codeit.codes/",
        "permanent": true
      },
      
      {
        "source": "/:path((?!_/).*)",
        "destination": "https://dev.codeit.codes/api/link?url=:path*",
        "permanent": true
      },
      {
        "source": "/:path((?!_/).*)/",
        "destination": "https://dev.codeit.codes/api/link?url=:path*",
        "permanent": true
      },
      
      {
        "source": "/_/twitter",
        "destination": "https://twitter.com/codeitcodes",
        "permanent": true
      }
   ]
}
