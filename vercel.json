{
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/_/run/:path*",
      "destination": ":path*"
    }
  ],
  "redirects": [
      {
        "source": "/",
        "destination": "https://dev.codeit.codes/",
        "permanent": true
      },
      {
        "source": "/:path((!_).*)",
        "destination": "https://dev.codeit.codes/api/link?url=:path*",
        "permanent": true
      },
      {
        "source": "/:path*/",
        "destination": "https://dev.codeit.codes/api/link?url=:path*",
        "permanent": true
      }
   ]
}
